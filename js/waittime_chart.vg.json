{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 300,
    "height": 300,
    "data": {
      "url": "https://raw.githubusercontent.com/danpham02/fit3179/main/data/Emergency-Department-Care-2021-22-waittime.csv",
      "format": {"type": "csv"}
    },
    "transform": [
      {
        "fold": ["Median waiting time (minutes)", "90th percentile waiting time (minutes)"],
        "as": ["Type", "Waiting Time"]
      },
      {
        "lookup": "Type",
        "from": {
          "data": {
            "values": [
              {"Type": "Median waiting time (minutes)", "NewType": "Median"},
              {"Type": "90th percentile waiting time (minutes)", "NewType": "90th Percentile"}
            ]
          },
          "key": "Type",
          "fields": ["NewType"]
        },
        "as": ["Type"]
      }
    ],
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "State",
            "type": "nominal",
            "axis": {
                "title": "State or Territory",
                "labelAngle": -45
            }
        },
        "xOffset": {
            "field": "Type"
        },
        "y": {
        "field": "Waiting Time",
        "type": "quantitative",
        "axis": {"title": "Waiting Time (Minutes)"}
      },
      "color": {
        "field": "Type",
        "type": "nominal",
        "legend": {"title": "Waiting Time Type"},
        "scale": {"range": ["#8E44AD", "#1ABC9C"]} 
      },
      "tooltip": [
        {"field": "State", "type": "nominal", "title": "State"},
        {"field": "Type", "type": "nominal", "title": "Waiting Time Type (Minutes)"},
        {"field": "Waiting Time", "type": "quantitative", "title": "Time (Minutes)"}
      ]
    },
    "title": {
      "text": "Comparison of Median and 90th Percentile Waiting Times",
      "subtitle": "Across States for Emergency Presentations (2021-22)",
      "fontSize": 16,
      "fontWeight": "bold",
      "anchor": "middle"
    }
  }
  