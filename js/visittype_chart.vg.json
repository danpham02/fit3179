{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/danpham02/fit3179/main/data/Emergency-Department-Care-2021-22-visittype.csv",
      "format": {"type": "csv"}
    },
    "transform": [
      {
        "fold": ["Emergency presentation", "Return visit, planned", "Pre-arranged admission", "Dead on arrival", "Not reported"],
        "as": ["Type", "Presentations"]
      }
    ],
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "State",
        "type": "nominal",
        "axis": {"title": "State or Territory"}
      },
      "y": {
        "field": "Presentations",
        "type": "quantitative",
        "axis": {"title": "Number of Presentations"}
      },
      "color": {
        "field": "Type",
        "type": "nominal",
        "legend": {"title": "Type of Visit"},
        "scale": {"range": ["#FFB6C1", "#4682B4", "#FFD700", "#FF6347", "#98FB98"]}
      },
      "tooltip": [
        {"field": "State", "type": "nominal", "title": "State"},
        {"field": "Type", "type": "nominal", "title": "Type of Visit"},
        {"field": "Presentations", "type": "quantitative", "title": "Number of Presentations"}
      ]
    },
    "title": {
      "text": "Emergency Department Presentations by Type of Visit",
      "subtitle": "Across States and Territories (2021-22)",
      "fontSize": 16,
      "fontWeight": "bold",
      "anchor": "middle"
    }
  }
  